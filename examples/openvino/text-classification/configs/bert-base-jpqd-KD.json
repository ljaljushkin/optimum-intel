[
    {
        "algorithm": "quantization",
        "preset": "mixed",
        "overflow_fix": "disable",
        "initializer": {
            "range": {"num_init_samples": 300, "type": "mean_min_max"},
            "batchnorm_adaptation": {"num_bn_adaptation_samples": 0}
        },
        "scope_overrides": {"activations": {"{re}.*matmul_0": {"mode": "symmetric"}}},
        "ignored_scopes": [
            "{re}.*Embedding*",
            "{re}.*__add___[0-1]",
            "{re}.*layer_norm_0",
            "{re}.*matmul_1",
            "{re}.*__truediv__*"
        ]
    },
    {
        "algorithm": "knowledge_distillation",
        "type": "none",
	//"scale": 3,
	//"temperature": 10,
        //"a_scopes": "{re}.*BertAttention\\[attention\\]/BertSelfAttention\\[self\\]/__add___0",
        "h_scopes": "{re}.*BertOutput\\[output\\]/NNCFLayerNorm\\[LayerNorm\\]/layer_norm_0"
    }
]

